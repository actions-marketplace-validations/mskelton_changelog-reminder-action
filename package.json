{
  "name": "changelog-reminder-action",
  "version": "0.1.0",
  "private": true,
  "description": "Github action to remind users their pull request is missing a changelog.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mskelton/changelog-reminder-action.git"
  },
  "keywords": [
    "actions",
    "changelog",
    "reminder"
  ],
  "author": "Mark Skelton",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "build": "ncc build",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "@mskelton",
      "@mskelton/eslint-config/typescript"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/github": "^2.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@mskelton/eslint-config": "^5.9.1",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.1.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.1.0",
    "eslint-plugin-sort": "^1.4.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  }
}
