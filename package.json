{
  "name": "changelog-reminder-action",
  "version": "0.1.0",
  "private": true,
  "description": "Github action to remind users their pull request is missing a changelog.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mskelton/changelog-reminder-action.git"
  },
  "keywords": [
    "actions",
    "changelog",
    "reminder"
  ],
  "author": "Mark Skelton",
  "license": "ISC",
  "main": "src/index.ts",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "build": "ncc build",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "@mskelton",
      "@mskelton/eslint-config/typescript",
      "@mskelton/eslint-config/jest"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@mskelton/eslint-config": "^5.12.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.13.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-sort": "^1.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  }
}
